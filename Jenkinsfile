pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Build Script'
        echo 'Building the projet'
      }
    }
    stage('Start Unit Test') {
      steps {
        echo 'Start Unit Testing'
      }
    }
    stage('Start Selenium Test') {
      steps {
        echo 'Selenium Test'
      }
    }
    stage('Start Load Test ') {
      steps {
        echo 'Start Loading Test '
      }
    }
    stage('Generate Test Report') {
      steps {
        echo 'generate Test Report'
      }
    }
    stage('Deploy in Test System') {
      steps {
        echo 'Deploy in Test System'
      }
    }
  }
}